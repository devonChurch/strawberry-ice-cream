{
    "extends": ["airbnb-base", "eslint:recommended"],
    "parserOptions": {
        "ecmaVersion": 6,
    },
    "env": {
        "browser": true,
        "node": true,
    },
    "rules": {
        "no-console": 0,
        "arrow-body-style": 0,
        "padded-blocks": 0,
        "no-underscore-dangle": 0,
        "no-unused-vars": 1,
        "one-var": 0,
        "max-len": 0,
        "no-confusing-arrow": 1,
        "no-return-assign": 0,
        "one-var-declaration-per-line": 0,
        "no-param-reassign": 0,
        "indent": 0,
        "comma-dangle": 0,
        "object-curly-spacing": 0,
        "react/prop-types": 0,
    }
}
